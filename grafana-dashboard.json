{
  "dashboard": {
    "title": "LocalZure Service Bus Monitoring",
    "tags": ["localzure", "servicebus", "azure"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Message Throughput (Messages/sec)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(servicebus_messages_sent_total[1m])",
            "legendFormat": "Sent - {{entity_name}}",
            "refId": "A"
          },
          {
            "expr": "rate(servicebus_messages_received_total[1m])",
            "legendFormat": "Received - {{entity_name}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Messages/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Active Messages by Entity",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "servicebus_active_messages",
            "legendFormat": "{{entity_type}}/{{entity_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Messages"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Message Send Latency (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(servicebus_send_duration_seconds_bucket[1m]))",
            "legendFormat": "P50 - {{entity_name}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(servicebus_send_duration_seconds_bucket[1m]))",
            "legendFormat": "P95 - {{entity_name}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(servicebus_send_duration_seconds_bucket[1m]))",
            "legendFormat": "P99 - {{entity_name}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Message Receive Latency (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(servicebus_receive_duration_seconds_bucket[1m]))",
            "legendFormat": "P50 - {{entity_name}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(servicebus_receive_duration_seconds_bucket[1m]))",
            "legendFormat": "P95 - {{entity_name}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(servicebus_receive_duration_seconds_bucket[1m]))",
            "legendFormat": "P99 - {{entity_name}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Error Rate (Errors/sec)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(servicebus_errors_total[1m])",
            "legendFormat": "{{operation}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "alert": {
          "name": "High Error Rate",
          "conditions": [
            {
              "evaluator": {"params": [0.05], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "1m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "message": "Error rate exceeds 5% threshold",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 6,
        "title": "Dead-Letter Messages",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "servicebus_deadletter_messages",
            "legendFormat": "{{entity_type}}/{{entity_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Messages"},
          {"format": "short"}
        ],
        "alert": {
          "name": "Dead-Letter Threshold",
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "message": "Dead-letter queue has more than 10 messages",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 7,
        "title": "Message Operations",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(servicebus_messages_completed_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "displayName": "Completed/sec"
          }
        }
      },
      {
        "id": 8,
        "title": "Abandoned Messages",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(servicebus_messages_abandoned_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "displayName": "Abandoned/sec",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.1, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Active Locks",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum(servicebus_active_locks)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "displayName": "Active Locks"
          }
        }
      },
      {
        "id": 10,
        "title": "Entity Count",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
        "targets": [
          {
            "expr": "servicebus_entity_count{entity_type=\"queue\"}",
            "legendFormat": "Queues",
            "refId": "A"
          },
          {
            "expr": "servicebus_entity_count{entity_type=\"topic\"}",
            "legendFormat": "Topics",
            "refId": "B"
          },
          {
            "expr": "servicebus_entity_count{entity_type=\"subscription\"}",
            "legendFormat": "Subscriptions",
            "refId": "C"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 11,
        "title": "Average Message Size",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(servicebus_message_size_bytes_bucket[1m]))",
            "legendFormat": "P50 - {{entity_name}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(servicebus_message_size_bytes_bucket[1m]))",
            "legendFormat": "P95 - {{entity_name}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Message Size"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "Lock Wait Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(servicebus_lock_wait_seconds_bucket[1m]))",
            "legendFormat": "P50 - {{entity_name}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(servicebus_lock_wait_seconds_bucket[1m]))",
            "legendFormat": "P95 - {{entity_name}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Wait Time"},
          {"format": "short"}
        ]
      }
    ]
  }
}
